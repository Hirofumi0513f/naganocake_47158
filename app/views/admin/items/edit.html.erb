<h4>商品編集</h4>
<%= form_with model: @itemf do |f| %>
<div>
  <label>商品画像</label>
</div>
<div>
  <label>商品名</label>
  <%= f.text_field :name %>
</div>
<div>
  <label>商品説明</label>
  <%= f.text_area :introduction %>
</div>
<div>
  <label>ジャンル</label>
</div>
<div>
  <label>税抜価格</label>
  <%= f.text_field :price %> 円
</div>
<div>
  <label>販売ステータス</label>
</div>
<div>
  <%= f.submit "変更を保存", class: 'btn btn-success' %>
</div>
<% end %>
